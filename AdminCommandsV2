local SpamController = false
while wait(1) do
local plr = game.Players.LocalPlayer
local chr = plr.Character




if SpamController == false then

local ParentGui = game.CoreGui
local ScreenGui = Instance.new("ScreenGui")
local TextButton1 = Instance.new("TextButton")
ScreenGui.Name = "SOCommandsGui"

ScreenGui.Parent = ParentGui

TextButton1.Name = "T1"


TextButton1.Parent = ScreenGui








TextButton1:TweenPosition(UDim2.new(0,144,0,50),"Out","Sine",1)
TextButton1:TweenSize(UDim2.new(0,120,0,90),"Out","Sine",1)
TextButton1.BackgroundTransparency = 0.6
TextButton1.TextTransparency = 0.5
TextButton1.BackgroundColor3 = Color3.fromRGB(7,230,51)
TextButton1.TextColor3 = Color3.fromRGB(0,0,0)


TextButton1.Text = "Turn Off Commands"


TextButton1.TextScaled = true



TextButton1.MouseButton1Click:Connect(function()
if TextButton1.Text == "Turn Off Commands" then
TextButton1.Text = "Turn On Commands"
else
TextButton1.Text = "Turn Off Commands"
end
end)


wait(0.1)
SpamController = true

end
plr.Chatted:Connect(function(msg)
if game.CoreGui.SOCommandsGui.T1.Text == "Turn Off Commands" then

if chr then
local rootpart = chr:WaitForChild("HumanoidRootPart")
local Humanoid = chr:WaitForChild("Humanoid")


if Humanoid then

if msg == "!Kill" then

Humanoid.Health = 0
end

if msg == "!Barrier" then


local Part = Instance.new("Part")
Part.Parent = game.Workspace
Part.Name = "MyPart"
Part.Anchored = true
Part.Transparency = 0.7
Part.Size = Vector3.new(10000,1,10000)
Part.CFrame = rootpart.CFrame - Vector3.new(0,7,0)
-- Press Execute

end

if msg == "!BarrierDestroy" then

game.Workspace.MyPart:Destroy()

end

local Blacklist = {

"!Barrier",
"!BarrierDestroy",
"!UnBarrier",
"!Barier",
"!BarierDestoy",
"!BarierDestroy",
"!BarrierDestoy",
"Barrier",
"BarrierDestroy"


}
for _,v in ipairs(Blacklist) do

KelimeSaptama = string.split(msg, " ")


if KelimeSaptama[1] == "!Speed" then
Humanoid.WalkSpeed = tonumber(KelimeSaptama[2])
end

if KelimeSaptama[1] == "!goto" then
local PlayersCharacter = game.Workspace[KelimeSaptama[2]]
local PlayersRoot = PlayersCharacter:FindFirstChild("HumanoidRootPart")
rootpart.CFrame = PlayersRoot.CFrame
end

end

end
end
end
end)
end
