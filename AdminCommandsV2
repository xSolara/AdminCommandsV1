
while wait(1) do
local plr = game.Players.LocalPlayer
local chr = plr.Character





plr.Chatted:Connect(function(msg)


if chr then
local rootpart = chr:WaitForChild("HumanoidRootPart")
local Humanoid = chr:WaitForChild("Humanoid")


if Humanoid then

if msg == "!Kill" then

Humanoid.Health = 0
end

if msg == "!Barrier" then


local Part = Instance.new("Part")
Part.Parent = game.Workspace
Part.Name = "MyPart"
Part.Anchored = true
Part.Transparency = 0.7
Part.Size = Vector3.new(10000,1,10000)
Part.CFrame = rootpart.CFrame - Vector3.new(0,7,0)
-- Press Execute

end

if msg == "!BarrierDestroy" then

game.Workspace.MyPart:Destroy()

end

local Blacklist = {

"!Barrier",
"!BarrierDestroy",
"!UnBarrier",
"!Barier",
"!BarierDestoy",
"!BarierDestroy",
"!BarrierDestoy",
"Barrier",
"BarrierDestroy"


}
for _,v in ipairs(Blacklist) do

KelimeSaptama = string.split(msg, " ")

if KelimeSaptama[1] == "!JumpPower" then
Humanoid.JumpPower = tonumber(KelimeSaptama[2])
end

if KelimeSaptama[1] == "!Speed" then
Humanoid.WalkSpeed = tonumber(KelimeSaptama[2])
end

if KelimeSaptama[1] == "!goto" then
local PlayersCharacter = game.Workspace[KelimeSaptama[2]]
local PlayersRoot = PlayersCharacter:FindFirstChild("HumanoidRootPart")
rootpart.CFrame = PlayersRoot.CFrame
end

end

end
end

end)
end
