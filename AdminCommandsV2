local ClickForXray = false
local NoclipEnabled = false
while wait(1) do
local plr = game.Players.LocalPlayer
local chr = plr.Character
local mouse = plr:GetMouse()

local function Noclip()

while wait(0.8) do
if NoclipEnabled == true then
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if v:IsA("BasePart") or v:IsA("MeshPart") or v:IsA("UnionOperation") or v:IsA("TrussPart") then

v.CanCollide = false
v.CanQuery = false
v.CanTouch = false

end
end
end
end
end
local function UnNoclip()


if NoclipEnabled == false then
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if v:IsA("BasePart") or v:IsA("MeshPart") or v:IsA("UnionOperation") or v:IsA("TrussPart") then

v.CanCollide = true
v.CanQuery = true
v.CanTouch = true


end
end
end
end


plr.Chatted:Connect(function(msg)


if chr then
local rootpart = chr:WaitForChild("HumanoidRootPart")
local Humanoid = chr:WaitForChild("Humanoid")


if Humanoid then

if msg == "!Kill" then

Humanoid.Health = 0
end

if msg == "!ClickForXray" then
ClickForXray = true
if ClickForXray == true then
mouse.Button1Down:Connect(function()
local instance = mouse.Target
if instance:IsA("BasePart") and instance.Transparency ~= 0.6 and instance.CanCollide ~= false then
instance.Transparency = 0.6
instance.CanCollide = false
else
instance.CanCollide = true
instance.Transparency = 0
print(instance)
end
end)
end
end


if msg == "!UnClickForXray" then

ClickForXray = false

end

if msg == "!Noclip" then
NoclipEnabled = true

end

if msg == "!UnNoclip" then
NoclipEnabled = false
UnNoclip()
end
if NoclipEnabled == true then

Noclip()
end


if msg == "!Barrier" then


local Part = Instance.new("Part")
Part.Parent = game.Workspace
Part.Name = "MyPart"
Part.Anchored = true
Part.Transparency = 0.7
Part.Size = Vector3.new(10000,1,10000)
Part.CFrame = rootpart.CFrame - Vector3.new(0,7,0)
-- Press Execute

end

if msg == "!BarrierDestroy" then

game.Workspace.MyPart:Destroy()

end

local Blacklist = {

"!Barrier",
"!BarrierDestroy",
"!UnBarrier",
"!Barier",
"!BarierDestoy",
"!BarierDestroy",
"!BarrierDestoy",
"Barrier",
"BarrierDestroy"


}
for _,v in ipairs(Blacklist) do

KelimeSaptama = string.split(msg, " ")

if KelimeSaptama[1] == "!JumpPower" then
Humanoid.JumpPower = tonumber(KelimeSaptama[2])
end

if KelimeSaptama[1] == "!Speed" then
Humanoid.WalkSpeed = tonumber(KelimeSaptama[2])
end

if KelimeSaptama[1] == "!goto" then
local PlayersCharacter = game.Workspace[KelimeSaptama[2]]
local PlayersRoot = PlayersCharacter:FindFirstChild("HumanoidRootPart")
rootpart.CFrame = PlayersRoot.CFrame
end

end

end
end

end)
end
